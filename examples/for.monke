let catalog = [
    {"name": "alpha", "values": [1, 2, 3, 4]},
    {"name": "beta", "values": [5, 6, 7, 8]},
    {"name": "gamma", "values": [9, 10, 11, 12]}
];

let grandTotal = 0;

for (let i = 0; i < len(catalog); i += 1) {
    let entry = catalog[i];
    let values = entry["values"];
    for (let j = 0; j < len(values); j += 1) {
        grandTotal = grandTotal + values[j];
    }
};

print(grandTotal);